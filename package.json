{
  "private": true,
  "name": "voxylstats",
  "description": "",
  "license": "",
  "scripts": {
    "build": "cross-env NODE_ENV=production remix build",
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "postinstall": "remix setup cloudflare-pages && patch-package",
    "dev:remix": "remix watch",
    "dev:wrangler": "source $PWD/.env && wrangler pages dev ./public --binding API_KEY=$API_KEY --kv MINECRAFT_UUIDS --kv VOXYL_CACHE --watch ./build",
    "start": "npm run dev:wrangler"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.6",
    "@remix-run/cloudflare-pages": "^1.1.0",
    "@remix-run/react": "^1.1.0",
    "js-convert-case": "^4.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.0",
    "tiny-invariant": "^1.2.0",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.2.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@remix-run/dev": "^1.1.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "cross-env": "^7.0.3",
    "esbuild": "0.13.14",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "typescript": "^4.1.2",
    "wrangler": "^0.0.0-8ab7d2e"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "dist/worker.js"
}
